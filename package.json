{
  "name": "expo-with-webview",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "deploy:ios": "eas build --platform ios && eas submit -p ios --latest",
    "deploy:android": "eas build --platform android && eas submit -p android --latest",
    "build:android-aab": "eas build -p android --local",
    "build:android-apk": "eas build -p android --profile preview --local"
  },
  "dependencies": {
    "@expo/webpack-config": "^18.0.1",
    "expo": "~48.0.18",
    "expo-clipboard": "~4.1.2",
    "expo-contacts": "~12.0.1",
    "expo-dev-client": "~2.2.1",
    "expo-device": "~5.2.1",
    "expo-linking": "~4.0.1",
    "expo-location": "~15.1.1",
    "expo-notifications": "~0.18.1",
    "expo-splash-screen": "~0.18.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-safe-area-context": "4.5.0",
    "react-native-web": "~0.18.10",
    "react-native-webview": "11.26.0",
    "expo-status-bar": "~1.4.4",
    "@react-native-async-storage/async-storage": "1.17.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
